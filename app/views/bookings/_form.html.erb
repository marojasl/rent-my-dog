<%= simple_form_for [@dog, @booking] do |f| %>

  <div class="form-inputs">
    <%= f.input :start_date, as: :date %>
  </div>

  <div class="form-inputs">
    <%= f.input :end_date, as: :date %>
  </div>

  <div class="form-inputs">
    <%= f.input :comment %>
  </div>

  <div class="form-actions btn btn-outline-success">
    <%= f.button :submit %>
  </div>
<% end %>
