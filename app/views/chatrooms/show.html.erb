<h5>Chat between <%= @chatroom.sender.first_name %> and <%= @chatroom.receiver.first_name %></h5>
<% @messages.each do |message| %>
  <p><%= message.message %> by <%= message.user.first_name %></p>
<% end %>
<br>
<br>
<%= simple_form_for([@chatroom, Message.new]) do |f| %>
  <%= f.input :message %>
  <%= f.submit "Send Message" %>
<% end %>
